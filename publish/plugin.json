{
  "pluginName": "文本生成器",
  "description": "快速将复制的文本生成txt文件保存到桌面",
  "author": "HUHA",
  "homepage": "",
  "version": "1.0.0",
  "logo": "logo.png",
  "main": "index.html",
  "preload": "preload.js",
  "features": [
    {
      "code": "save_desktop",
      "explain": "保存到桌面",
      "cmds": [
        "文本生成器 桌面",
        "保存为txt到桌面",
        "生成txt到桌面",
        {
          "type": "over",
          "label": "保存剪贴板为txt到桌面",
          "match": {
            "clipboard": {
              "type": "text",
              "minLength": 1
            }
          }
        }
      ]
    },
    {
      "code": "save_custom_name",
      "explain": "自定义文件名保存",
      "cmds": [
        "文本生成器 自定义文件名",
        "保存txt 自定义文件名",
        "生成txt 自定义文件名"
      ]
    },
    {
      "code": "save_custom_path",
      "explain": "选择文件夹保存",
      "cmds": [
        "文本生成器 选择文件夹",
        "保存txt 选择文件夹",
        "生成txt 选择文件夹"
      ]
    },
    {
      "code": "save_custom_both",
      "explain": "选择路径保存",
      "cmds": [
        "文本生成器 选择路径",
        "保存txt 选择路径",
        "生成txt 选择路径"
      ]
    }
  ]
}
